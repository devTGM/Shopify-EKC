{% doc %}
  @prompt
    Create a premium testimonial carousel section for a Shopify homepage.
    
    The section should feature:
    
    A centered headline and short subheading at the top
    
    A horizontally scrolling carousel of customer testimonials
    
    Each testimonial card includes:
    
    Customer lifestyle image or video
    
    Short quote (1–2 lines max)
    
    Customer name
    
    Linked product name
    
    Carousel behavior:
    
    Auto-scrolls smoothly from right to left
    
    Infinite loop
    
    Pauses on hover (desktop)
    
    Swipeable on mobile
    
    Shows 3–4 cards on desktop, 1 card on mobile
    
    Design style:
    
    Minimal, luxury aesthetic
    
    White or soft neutral background
    
    Clean typography using body font
    
    No heavy borders
    
    Soft shadows or none
    
    Ensure the section is fully responsive, fast-loading, and accessible., I need video volume control option, Need border radius option and Aspect ratio control option too, i Want 14 testimonials and don't wrap the quote, I want to show the complete quote
    
    Also I want the carousel can be controlled with cursor(on desktop) and touch(on Mobile)
{% enddoc %}
{% assign ai_gen_id = block.id | replace: '_', '' | downcase %}

{% style %}
  .ai-testimonial-carousel-{{ ai_gen_id }} {
    display: block;
    width: 100%;
    background-color: {{ block.settings.background_color }};
    padding: {{ block.settings.section_padding_top }}px 0 {{ block.settings.section_padding_bottom }}px;
    overflow: hidden;
  }

  .ai-testimonial-carousel__header-{{ ai_gen_id }} {
    text-align: center;
    margin: 0 auto {{ block.settings.header_spacing }}px;
    max-width: 800px;
    padding: 0 20px;
  }

  .ai-testimonial-carousel__heading-{{ ai_gen_id }} {
    font-family: {{ block.settings.heading_font.family }}, {{ block.settings.heading_font.fallback_families }};
    font-weight: {{ block.settings.heading_font.weight }};
    font-size: {{ block.settings.heading_size }}px;
    color: {{ block.settings.heading_color }};
    margin: 0 0 {{ block.settings.subheading_spacing }}px;
    line-height: 1.2;
  }

  .ai-testimonial-carousel__subheading-{{ ai_gen_id }} {
    font-family: {{ block.settings.body_font.family }}, {{ block.settings.body_font.fallback_families }};
    font-weight: {{ block.settings.body_font.weight }};
    font-size: {{ block.settings.subheading_size }}px;
    color: {{ block.settings.text_color }};
    margin: 0;
    line-height: 1.5;
  }

  .ai-testimonial-carousel__track-wrapper-{{ ai_gen_id }} {
    position: relative;
    width: 100%;
    overflow: hidden;
    cursor: grab;
    user-select: none;
  }

  .ai-testimonial-carousel__track-wrapper-{{ ai_gen_id }}.is-dragging {
    cursor: grabbing;
  }

  .ai-testimonial-carousel__track-{{ ai_gen_id }} {
    display: flex;
    gap: {{ block.settings.card_gap }}px;
    will-change: transform;
    transition: transform 0.1s linear;
  }

  .ai-testimonial-carousel__track-{{ ai_gen_id }}.is-animating {
    transition: transform 0.5s linear;
  }

  .ai-testimonial-carousel__card-{{ ai_gen_id }} {
    flex: 0 0 auto;
    width: 100%;
    background-color: {{ block.settings.card_background }};
    border-radius: {{ block.settings.card_border_radius }}px;
    overflow: hidden;
    {% if block.settings.card_shadow %}
      box-shadow: 0 2px 12px rgba(0, 0, 0, 0.08);
    {% endif %}
  }

  .ai-testimonial-carousel__media-{{ ai_gen_id }} {
    position: relative;
    width: 100%;
    aspect-ratio: {{ block.settings.media_aspect_ratio }};
    overflow: hidden;
    background-color: #f5f5f5;
  }

  .ai-testimonial-carousel__media-{{ ai_gen_id }} img,
  .ai-testimonial-carousel__media-{{ ai_gen_id }} video {
    width: 100%;
    height: 100%;
    object-fit: cover;
    display: block;
  }

  .ai-testimonial-carousel__media-placeholder-{{ ai_gen_id }} {
    width: 100%;
    height: 100%;
    display: flex;
    align-items: center;
    justify-content: center;
    background-color: #f5f5f5;
  }

  .ai-testimonial-carousel__media-placeholder-{{ ai_gen_id }} svg {
    width: 60%;
    height: 60%;
    opacity: 0.3;
  }

  .ai-testimonial-carousel__video-controls-{{ ai_gen_id }} {
    position: absolute;
    bottom: 10px;
    right: 10px;
    z-index: 2;
  }

  .ai-testimonial-carousel__volume-btn-{{ ai_gen_id }} {
    background-color: rgba(255, 255, 255, 0.9);
    border: none;
    border-radius: 50%;
    width: 32px;
    height: 32px;
    display: flex;
    align-items: center;
    justify-content: center;
    cursor: pointer;
    transition: background-color 0.2s;
  }

  .ai-testimonial-carousel__volume-btn-{{ ai_gen_id }}:hover {
    background-color: rgba(255, 255, 255, 1);
  }

  .ai-testimonial-carousel__volume-btn-{{ ai_gen_id }} svg {
    width: 16px;
    height: 16px;
  }

  .ai-testimonial-carousel__content-{{ ai_gen_id }} {
    padding: {{ block.settings.card_padding }}px;
  }

  .ai-testimonial-carousel__quote-{{ ai_gen_id }} {
    font-family: {{ block.settings.body_font.family }}, {{ block.settings.body_font.fallback_families }};
    font-weight: {{ block.settings.body_font.weight }};
    font-size: {{ block.settings.quote_size }}px;
    color: {{ block.settings.text_color }};
    line-height: 1.6;
    margin: 0 0 {{ block.settings.quote_spacing }}px;
    white-space: normal;
  }

  .ai-testimonial-carousel__customer-{{ ai_gen_id }} {
    font-family: {{ block.settings.body_font.family }}, {{ block.settings.body_font.fallback_families }};
    font-weight: 600;
    font-size: {{ block.settings.name_size }}px;
    color: {{ block.settings.heading_color }};
    margin: 0 0 {{ block.settings.name_spacing }}px;
  }

  .ai-testimonial-carousel__product-link-{{ ai_gen_id }} {
    font-family: {{ block.settings.body_font.family }}, {{ block.settings.body_font.fallback_families }};
    font-weight: {{ block.settings.body_font.weight }};
    font-size: {{ block.settings.product_size }}px;
    color: {{ block.settings.link_color }};
    text-decoration: none;
    transition: color 0.2s;
    display: inline-block;
  }

  .ai-testimonial-carousel__product-link-{{ ai_gen_id }}:hover {
    color: {{ block.settings.link_hover_color }};
  }

  @media screen and (min-width: 750px) {
    .ai-testimonial-carousel__heading-{{ ai_gen_id }} {
      font-size: {{ block.settings.heading_size | times: 1.2 }}px;
    }

    .ai-testimonial-carousel__subheading-{{ ai_gen_id }} {
      font-size: {{ block.settings.subheading_size | times: 1.1 }}px;
    }

    .ai-testimonial-carousel__card-{{ ai_gen_id }} {
      width: calc((100% - ({{ block.settings.card_gap }}px * ({{ block.settings.cards_desktop }} - 1))) / {{ block.settings.cards_desktop }});
    }

    .ai-testimonial-carousel__track-wrapper-{{ ai_gen_id }}:hover .ai-testimonial-carousel__track-{{ ai_gen_id }}.is-animating {
      animation-play-state: paused;
    }
  }

  @media screen and (max-width: 749px) {
    .ai-testimonial-carousel__content-{{ ai_gen_id }} {
      padding: {{ block.settings.card_padding | times: 0.8 }}px;
    }
  }
{% endstyle %}

<testimonial-carousel-{{ ai_gen_id }}
  class="ai-testimonial-carousel-{{ ai_gen_id }}"
  data-auto-scroll="{{ block.settings.auto_scroll }}"
  data-scroll-speed="{{ block.settings.scroll_speed }}"
  {{ block.shopify_attributes }}
>
  {% if block.settings.heading != blank or block.settings.subheading != blank %}
    <div class="ai-testimonial-carousel__header-{{ ai_gen_id }}">
      {% if block.settings.heading != blank %}
        <h2 class="ai-testimonial-carousel__heading-{{ ai_gen_id }}">
          {{ block.settings.heading }}
        </h2>
      {% endif %}
      {% if block.settings.subheading != blank %}
        <p class="ai-testimonial-carousel__subheading-{{ ai_gen_id }}">
          {{ block.settings.subheading }}
        </p>
      {% endif %}
    </div>
  {% endif %}

  <div class="ai-testimonial-carousel__track-wrapper-{{ ai_gen_id }}">
    <div class="ai-testimonial-carousel__track-{{ ai_gen_id }}">
      {% for i in (1..14) %}
        {% liquid
          assign quote_key = 'quote_' | append: i
          assign customer_key = 'customer_' | append: i
          assign product_key = 'product_' | append: i
          assign image_key = 'image_' | append: i
          assign video_key = 'video_' | append: i
          assign use_video_key = 'use_video_' | append: i

          assign quote = block.settings[quote_key]
          assign customer = block.settings[customer_key]
          assign product = block.settings[product_key]
          assign image = block.settings[image_key]
          assign video = block.settings[video_key]
          assign use_video = block.settings[use_video_key]
        %}

        <div class="ai-testimonial-carousel__card-{{ ai_gen_id }}">
          <div class="ai-testimonial-carousel__media-{{ ai_gen_id }}">
            {% if use_video and video != blank %}
              <video
                class="ai-testimonial-carousel__video-{{ ai_gen_id }}"
                src="{{ video }}"
                loop
                muted
                playsinline
                autoplay
                data-video-index="{{ i }}"
              ></video>
              {% if block.settings.show_volume_control %}
                <div class="ai-testimonial-carousel__video-controls-{{ ai_gen_id }}">
                  <button
                    class="ai-testimonial-carousel__volume-btn-{{ ai_gen_id }}"
                    data-video-index="{{ i }}"
                    aria-label="Toggle volume"
                  >
                    <svg class="volume-on" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2">
                      <polygon points="11 5 6 9 2 9 2 15 6 15 11 19 11 5"></polygon>
                      <path d="M15.54 8.46a5 5 0 0 1 0 7.07"></path>
                    </svg>
                    <svg class="volume-off" style="display: none;" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2">
                      <polygon points="11 5 6 9 2 9 2 15 6 15 11 19 11 5"></polygon>
                      <line x1="23" y1="9" x2="17" y2="15"></line>
                      <line x1="17" y1="9" x2="23" y2="15"></line>
                    </svg>
                  </button>
                </div>
              {% endif %}
            {% elsif image != blank %}
              <img
                src="{{ image | image_url: width: 800 }}"
                alt="{{ customer | escape }}"
                loading="lazy"
                width="800"
                height="800"
              >
            {% else %}
              <div class="ai-testimonial-carousel__media-placeholder-{{ ai_gen_id }}">
                {{ 'lifestyle-1' | placeholder_svg_tag }}
              </div>
            {% endif %}
          </div>

          <div class="ai-testimonial-carousel__content-{{ ai_gen_id }}">
            {% if quote != blank %}
              <p class="ai-testimonial-carousel__quote-{{ ai_gen_id }}">
                "{{ quote }}"
              </p>
            {% endif %}

            {% if customer != blank %}
              <div class="ai-testimonial-carousel__customer-{{ ai_gen_id }}">
                {{ customer }}
              </div>
            {% endif %}

            {% if product != blank %}
              <a
                href="{{ product.url }}"
                class="ai-testimonial-carousel__product-link-{{ ai_gen_id }}"
              >
                {{ product.title }}
              </a>
            {% endif %}
          </div>
        </div>
      {% endfor %}
    </div>
  </div>
</testimonial-carousel-{{ ai_gen_id }}>

<script>
  (function() {
    class TestimonialCarousel{{ ai_gen_id }} extends HTMLElement {
      constructor() {
        super();
        this.trackWrapper = this.querySelector('.ai-testimonial-carousel__track-wrapper-{{ ai_gen_id }}');
        this.track = this.querySelector('.ai-testimonial-carousel__track-{{ ai_gen_id }}');
        this.cards = Array.from(this.querySelectorAll('.ai-testimonial-carousel__card-{{ ai_gen_id }}'));
        this.autoScroll = this.dataset.autoScroll === 'true';
        this.scrollSpeed = parseFloat(this.dataset.scrollSpeed) || 1;
        
        this.isDragging = false;
        this.startX = 0;
        this.currentTranslate = 0;
        this.prevTranslate = 0;
        this.animationId = null;
        this.lastTime = Date.now();
      }

      connectedCallback() {
        this.cloneCards();
        this.setupDragControls();
        this.setupVolumeControls();
        
        if (this.autoScroll) {
          this.startAutoScroll();
        }
      }

      cloneCards() {
        const clones = this.cards.map(card => card.cloneNode(true));
        clones.forEach(clone => this.track.appendChild(clone));
        
        const moreClones = this.cards.map(card => card.cloneNode(true));
        moreClones.forEach(clone => this.track.appendChild(clone));
      }

      setupDragControls() {
        this.trackWrapper.addEventListener('mousedown', this.dragStart.bind(this));
        this.trackWrapper.addEventListener('touchstart', this.dragStart.bind(this), { passive: true });
        
        this.trackWrapper.addEventListener('mousemove', this.drag.bind(this));
        this.trackWrapper.addEventListener('touchmove', this.drag.bind(this), { passive: true });
        
        this.trackWrapper.addEventListener('mouseup', this.dragEnd.bind(this));
        this.trackWrapper.addEventListener('mouseleave', this.dragEnd.bind(this));
        this.trackWrapper.addEventListener('touchend', this.dragEnd.bind(this));
        
        this.trackWrapper.addEventListener('mouseenter', () => {
          if (this.autoScroll) {
            this.pauseAutoScroll();
          }
        });
        
        this.trackWrapper.addEventListener('mouseleave', () => {
          if (this.autoScroll && !this.isDragging) {
            this.resumeAutoScroll();
          }
        });
      }

      setupVolumeControls() {
        const volumeButtons = this.querySelectorAll('.ai-testimonial-carousel__volume-btn-{{ ai_gen_id }}');
        volumeButtons.forEach(button => {
          button.addEventListener('click', (e) => {
            e.preventDefault();
            const videoIndex = button.dataset.videoIndex;
            const videos = this.querySelectorAll(`video[data-video-index="${videoIndex}"]`);
            const volumeOnIcon = button.querySelector('.volume-on');
            const volumeOffIcon = button.querySelector('.volume-off');
            
            videos.forEach(video => {
              video.muted = !video.muted;
            });
            
            if (videos[0] && videos[0].muted) {
              volumeOnIcon.style.display = 'none';
              volumeOffIcon.style.display = 'block';
            } else {
              volumeOnIcon.style.display = 'block';
              volumeOffIcon.style.display = 'none';
            }
          });
        });
      }

      dragStart(e) {
        this.isDragging = true;
        this.trackWrapper.classList.add('is-dragging');
        this.track.classList.remove('is-animating');
        
        this.startX = this.getPositionX(e);
        this.prevTranslate = this.currentTranslate;
        
        if (this.autoScroll) {
          this.pauseAutoScroll();
        }
      }

      drag(e) {
        if (!this.isDragging) return;
        
        const currentPosition = this.getPositionX(e);
        const diff = currentPosition - this.startX;
        this.currentTranslate = this.prevTranslate + diff;
        
        this.setTranslate();
      }

      dragEnd() {
        if (!this.isDragging) return;
        
        this.isDragging = false;
        this.trackWrapper.classList.remove('is-dragging');
        
        if (this.autoScroll) {
          this.resumeAutoScroll();
        }
      }

      getPositionX(e) {
        return e.type.includes('mouse') ? e.pageX : e.touches[0].clientX;
      }

      startAutoScroll() {
        this.track.classList.add('is-animating');
        this.animate();
      }

      animate() {
        if (!this.autoScroll || this.isDragging) return;
        
        const now = Date.now();
        const delta = now - this.lastTime;
        this.lastTime = now;
        
        this.currentTranslate -= this.scrollSpeed * (delta / 16);
        
        const cardWidth = this.cards[0].offsetWidth;
        const gap = parseInt(getComputedStyle(this.track).gap) || 0;
        const totalWidth = (cardWidth + gap) * this.cards.length;
        
        if (Math.abs(this.currentTranslate) >= totalWidth) {
          this.currentTranslate = 0;
        }
        
        this.setTranslate();
        this.animationId = requestAnimationFrame(() => this.animate());
      }

      setTranslate() {
        this.track.style.transform = `translateX(${this.currentTranslate}px)`;
      }

      pauseAutoScroll() {
        if (this.animationId) {
          cancelAnimationFrame(this.animationId);
          this.animationId = null;
        }
      }

      resumeAutoScroll() {
        if (!this.animationId) {
          this.lastTime = Date.now();
          this.animate();
        }
      }

      disconnectedCallback() {
        this.pauseAutoScroll();
      }
    }

    customElements.define('testimonial-carousel-{{ ai_gen_id }}', TestimonialCarousel{{ ai_gen_id }});
  })();
</script>

